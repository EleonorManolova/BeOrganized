{
  "version": "1.0",
  "defaultProvider": "jsdelivr",
  "libraries": [
    {
      "library": "jquery@3.4.1",
      "destination": "wwwroot/lib/jquery/",
      "files": [
        "dist/jquery.min.js",
        "dist/jquery.js"
      ]
    },
    {
      "library": "bootstrap@4.4.1",
      "destination": "wwwroot/lib/bootstrap/",
      "files": [
        "dist/css/bootstrap.css",
        "dist/css/bootstrap.min.css",
        "dist/js/bootstrap.js",
        "dist/js/bootstrap.min.js"
      ]
    },
    {
      "library": "jquery-validation@1.19.1",
      "destination": "wwwroot/lib/jquery-validation/",
      "files": [
        "dist/jquery.validate.js",
        "dist/jquery.validate.min.js"
      ]
    },
    {
      "library": "jquery-validation-unobtrusive@3.2.11",
      "destination": "wwwroot/lib/jquery-validation-unobtrusive/",
      "files": [
        "dist/jquery.validate.unobtrusive.js",
        "dist/jquery.validate.unobtrusive.min.js"
      ]
    },
    {
      // Full calendar Main
      "library": "@fullcalendar/core@4.4.0",
      "destination": "wwwroot/lib/node_modules/core/",
      "files": [
        "main.css",
        "main.min.css",
        "main.js",
        "main.min.js"
      ]
    },
    {
      // Full calendar DayGrid
      "library": "@fullcalendar/daygrid@4.4.0",
      "destination": "wwwroot/lib/node_modules/daygrid/",
      "files": [
        "main.css",
        "main.min.css",
        "main.js",
        "main.min.js"
      ]
    },
    {
      // Full calendar TimeGrid
      "library": "@fullcalendar/timegrid@4.4.0",
      "destination": "wwwroot/lib/node_modules/timegrid/",
      "files": [
        "main.css",
        "main.min.css",
        "main.js",
        "main.min.js"
      ]
    },
    {
      // Full calendar Interactions
      "library": "@fullcalendar/interaction@4.4.0",
      "destination": "wwwroot/lib/node_modules/interaction/",
      "files": [
        "main.js",
        "main.min.js"
      ]
    },
    {
      // Full calendar Resource Common
      "library": "@fullcalendar/resource-common@4.4.0",
      "destination": "wwwroot/lib/node_modules/resource-common/",
      "files": [
        "main.js",
        "main.min.js"
      ]
    },
    {
      // Full calendar Resource DayGrid
      "library": "@fullcalendar/resource-daygrid@4.4.0",
      "destination": "wwwroot/lib/node_modules/resource-daygrid/",
      "files": [
        "main.js",
        "main.min.js"
      ]
    },
    {
      // Full calendar Resource TimeGrid
      "library": "@fullcalendar/resource-timegrid@4.4.0",
      "destination": "wwwroot/lib/node_modules/resource-timegrid/",
      "files": [
        "main.js",
        "main.min.js"
      ]
    },
    {
      // QRCode Generator in Two-factor Authenticator
      "library": "qrcodejs@1.0.0",
      "destination": "wwwroot/lib/qrcode/"
    },
    {
      "library": "elasticsearch@16.6.0",
      "destination": "wwwroot/lib/elasticsearch",
      "files": [
        ".eslintrc.js",
        ".eslintrc.min.js"
      ]
    }
  ]
}